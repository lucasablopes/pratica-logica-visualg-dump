Algoritmo "EXERCICIONOTAS"

Var

   x,y : real
   
Inicio

   escreva("Nota do primeiro semestre: ")
   leia(x)
   escreva("Nota do segundo semestre: ")
   leia(y)
   escreval
   escreval("Sua nota final foi", x+y)
   escreval
   se x+y < 60.00 entao
      escreva("Você foi reprovado")
      senao
      escreva("Você foi aprovado")
   fimse

Fimalgoritmo